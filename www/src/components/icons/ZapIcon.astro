---
interface Props {
  width?: string;
  height?: string;
  class?: string;
}

const { width = "28", height = "28", class: className = "" } = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  width={width}
  height={height}
  viewBox="0 0 24 24"
  fill="none"
  stroke="currentColor"
  stroke-width="2"
  stroke-linecap="round"
  stroke-linejoin="round"
  class={className}
>
  <polygon points="13 2 3 14 12 14 11 22 21 10 12 10 13 2"/>
</svg>
